<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="4dp"  />
            <stroke android:width="1dip" android:color="#90955B" />
            <gradient android:angle="-90" android:startColor="#FFFF00" android:endColor="#C3C32E"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="4dp"  />
            <stroke android:width="1dip" android:color="#555521" />
            <solid android:color="#9C9C1F"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="4dp"  />
            <stroke android:width="1dip" android:color="#AFB42B" />
            <gradient android:angle="-90" android:startColor="#FFD740" android:endColor="#E3DA42" />
        </shape>
    </item>
</selector>
